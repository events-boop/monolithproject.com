    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.45.1",
    "embla-carousel-react": "^8.6.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.23.22",
    "helmet": "^8.1.0",
    "input-otp": "^1.4.2",
    "lenis": "^1.3.17",
    "lucide-react": "^0.453.0",
    "nanoid": "^5.1.5",
    "posthog-js": "^1.347.1",
    "react": "^19.2.1",
    "react-day-picker": "^9.11.1",
